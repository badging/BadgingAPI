# github OAuth app configs
GITHUB_AUTH_CLIENT_ID= # Your GitHub OAuth App Client ID
GITHUB_AUTH_CLIENT_SECRET= # Your GitHub OAuth App Client Secret

# github app configs
GITHUB_APP_ID= # Your GitHub App ID
GITHUB_APP_CLIENT_ID= # Your GitHub App Client ID
GITHUB_APP_CLIENT_SECRET= # Your GitHub App Client Secret
GITHUB_APP_WEBHOOK_SECRET= # Your GitHub App Webhook Secret
GITHUB_APP_PRIVATE_KEY= # Your GitHub App Private Key

# gitlab OAuth app configs
GITLAB_APP_CLIENT_ID= # Your GitLab OAuth App Client ID
GITLAB_APP_CLIENT_SECRET= # Your GitLab OAuth App Client Secret
GITLAB_APP_REDIRECT_URI= # Your GitLab OAuth App Redirect URI

# email configs
EMAIL_HOST= # Email service provider (e.g., Gmail)
EMAIL_ADDRESS= # Your email address
EMAIL_PASSWORD= # Your email app password (not your email account password). To get a unique Gmail app password, follow this link -> https://myaccount.google.com/apppasswords

PORT= # The port on which the server will run

# augur configs
AUGUR_APP_CLIENT_SECRET= # Your Augur App Client Secret. To get client secret, create an app via https://ai.chaoss.io/account/login

# database configs
DB_DIALECT= # Database dialect (default is 'mysql')
DB_HOST= # Database host (default is 'localhost')
DB_NAME= # Name of the database
DB_USER= # Database user
DB_PASSWORD= # Database user password

# smeeClient URL for testing
SMEE_CLIENT_URL= # Smee client URL for forwarding webhooks in development mode